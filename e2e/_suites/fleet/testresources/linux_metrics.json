{
  "inputs": [{
    "policy_template": "system",
    "streams": [{
      "data_stream": {
        "type": "metrics",
        "dataset": "linux.entropy"
      },
      "vars": {
        "period": {
          "type": "text",
          "value": "10s"
        }
      },
      "id": "system/metrics-linux.entropy-f0b065e3-1b62-416c-8998-93ea58fb98a1",
      "enabled": false
    },
      {
        "compiled_stream": {
          "condition": "${host.platform} == 'linux'",
          "period": "10s",
          "metricsets": [
            "network_summary"
          ],
          "processors": [{
            "drop_fields": {
              "fields": "event.module"
            }
          }]
        },
        "data_stream": {
          "type": "metrics",
          "dataset": "linux.network_summary"
        },
        "vars": {
          "period": {
            "type": "text",
            "value": "10s"
          }
        },
        "id": "system/metrics-linux.network_summary-f0b065e3-1b62-416c-8998-93ea58fb98a1",
        "enabled": true
      },
      {
        "compiled_stream": {
          "condition": "${host.platform} == 'linux'",
          "period": "10s",
          "metricsets": [
            "raid"
          ],
          "processors": [{
            "drop_fields": {
              "fields": "event.module"
            }
          }]
        },
        "data_stream": {
          "type": "metrics",
          "dataset": "linux.raid"
        },
        "vars": {
          "raid.mount_point": {
            "type": "text"
          },
          "period": {
            "type": "text",
            "value": "10s"
          }
        },
        "id": "system/metrics-linux.raid-f0b065e3-1b62-416c-8998-93ea58fb98a1",
        "enabled": true
      },
      {
        "compiled_stream": {
          "condition": "${host.platform} == 'linux'",
          "period": "10s",
          "service.state_filter": "active",
          "metricsets": [
            "service"
          ],
          "processors": [{
            "drop_fields": {
              "fields": "event.module"
            }
          }]
        },
        "data_stream": {
          "type": "metrics",
          "dataset": "linux.service"
        },
        "vars": {
          "service.pattern_filter": {
            "type": "text",
            "value": []
          },
          "period": {
            "type": "text",
            "value": "10s"
          },
          "service.state_filter": {
            "type": "text",
            "value": [
              "active"
            ]
          }
        },
        "id": "system/metrics-linux.service-f0b065e3-1b62-416c-8998-93ea58fb98a1",
        "enabled": true
      },
      {
        "compiled_stream": {
          "condition": "${host.platform} == 'linux'",
          "period": "10s",
          "metricsets": [
            "socket"
          ],
          "processors": [{
            "drop_fields": {
              "fields": "event.module"
            }
          }]
        },
        "data_stream": {
          "type": "metrics",
          "dataset": "linux.socket"
        },
        "vars": {
          "period": {
            "type": "text",
            "value": "10s"
          },
          "socket.reverse_lookup.failure_ttl": {
            "type": "text"
          },
          "socket.reverse_lookup.enabled": {
            "type": "bool",
            "value": false
          },
          "socket.reverse_lookup.success_ttl": {
            "type": "text"
          }
        },
        "id": "system/metrics-linux.socket-f0b065e3-1b62-416c-8998-93ea58fb98a1",
        "enabled": true
      },
      {
        "compiled_stream": {
          "condition": "${host.platform} == 'linux'",
          "period": "10s",
          "metricsets": [
            "users"
          ],
          "processors": [{
            "drop_fields": {
              "fields": "event.module"
            }
          }]
        },
        "data_stream": {
          "type": "metrics",
          "dataset": "linux.users"
        },
        "vars": {
          "period": {
            "type": "text",
            "value": "10s"
          }
        },
        "id": "system/metrics-linux.users-f0b065e3-1b62-416c-8998-93ea58fb98a1",
        "enabled": true
      }
    ],
    "vars": {
      "system.hostfs": {
        "type": "text"
      }
    },
    "type": "system/metrics",
    "enabled": true
  },
    {
      "policy_template": "system",
      "streams": [{
        "data_stream": {
          "type": "metrics",
          "dataset": "linux.conntrack"
        },
        "vars": {
          "period": {
            "type": "text",
            "value": "10s"
          }
